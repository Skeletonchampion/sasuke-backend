                //    document.getElementById("ctl00_phBody_RequestBook_txtQty").value,
                //    document.getElementById("ctl00_phBody_RequestBook_txtEmailReq").value,
                //    document.getElementById("ctl00_phBody_RequestBook_txtPhone").value, onFunctionSuccess);
            }
            return false;
        }
    }
    function ResendOTPEmail() {
        $('.full-loader').fadeIn();

        SearchResultService.ResendOTPEmail(document.getElementById("ctl00_Footer_ValidateOTP_hdnEmail").value, document.getElementById("ctl00_Footer_ValidateOTP_hdnType").value,  ValidateOTP_Success);
        return false;
      
     }
    function ValidateOTP_Success(res) {
        $('.full-loader').fadeOut();
        if (res == "") {
            alert("OOPs!! Some error occurred while resendign OTP.");
        }
        else {
            document.getElementById("ctl00_Footer_ValidateOTP_hdnOTP").value = res;
            alert("OTP has been send on your mention email id.");
