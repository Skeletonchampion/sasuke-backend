<script type="text/javascript">
   
    function ValidateOTP() {
        var otp = document.getElementById("ctl00_Footer_ValidateOTP_txtOTP").value.trim() + document.getElementById("ctl00_Footer_ValidateOTP_txtOTP1").value.trim() + document.getElementById("ctl00_Footer_ValidateOTP_txtOTP2").value.trim() + document.getElementById("ctl00_Footer_ValidateOTP_txtOTP3").value.trim() + document.getElementById("ctl00_Footer_ValidateOTP_txtOTP4").value.trim() + document.getElementById("ctl00_Footer_ValidateOTP_txtOTP5").value.trim();
        if (otp != document.getElementById("ctl00_Footer_ValidateOTP_hdnOTP").value) {
           
            alert("Please enter corrent OTP");
            return false;
        }
        else {
            document.getElementById("ctl00_Footer_ValidateOTP_hdnVerify").value = '1';
            alert("OTP validated");
            $("#otpmodal").modal('hide');
            if ((document.getElementById("ctl00_Footer_ValidateOTP_hdnType").value) == "contactus")
            {
                enableEmail();
            }
            else if((document.getElementById("ctl00_Footer_ValidateOTP_hdnType").value) == "notifyme")
            {
                verify = 1;
                if ((document.getElementById("ctl00_Footer_ValidateOTP_hdnPageType").value) == "productDetail") {
                    addNotifyMe();
                    OnCloseVerifyEmail(verify);
                }
                else {
                    pid = (document.getElementById("ctl00_Footer_ValidateOTP_productid").value);
                    addNotifyMe(pid, document.getElementById("ctl00_Footer_ValidateOTP_hdnIsbn").value, document.getElementById("email1").textContent);
                    OnCloseVerifyEmail(verify,pid);
                }
            }

            if ((document.getElementById("ctl00_Footer_ValidateOTP_hdnType").value) == "requestbook") {
              
                 enableEmail();
               // SearchResultService searchResultService = new SearchResultService();
                //SearchResultService.addrequestabook(document.getElementById("ctl00_phBody_RequestBook_txtTitle").value,
